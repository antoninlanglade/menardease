    "electron": "~1.6.2",
    "electron-packager": "^8.7.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-compat": "^1.0.3",
    "eslint-plugin-import": "^2.3.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-eslint": "^4.0.0",
