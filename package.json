  "description": "An easing tool to get the 2 anchor points values into clipboard",
  "main": "main.js",
  "scripts": {
    "start": "gulp",
    "build": "gulp dist && cd dist/ && electron .",
    "build:app" : "gulp dist && electron-packager ./dist/ --overwrite=true --out=builds"
