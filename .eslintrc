{
  "extends": [ "airbnb-base" ],
  "env": {
    "browser": true,
    "es6": false
  },
  "plugins": [
    "compat"
  ],
  "rules": {
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "class-methods-use-this": 0,
    "compat/compat": 2,
    "global-require": 0,
    "import/named": 2,
    "import/no-dynamic-require": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "max-len": [
      "error",
      {
        "code": 100,
        "ignoreUrls": true,
        "ignoreComments": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "new-cap": [
      2,
      {
        "capIsNew": false,
        "newIsCap": true
      }
    ],
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-tabs": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "no-use-before-define": ["error", { "variables": false, "functions": false, "classes": true }],
    "import/first": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "wrap-iife": ["error", "any"],
    "no-unused-expressions": 0,
    "func-names": 0,
    "no-mixed-operators": 0,
    "consistent-return": 0,
    "no-bitwise": 0,
    "no-plusplus": 0
  },
  "settings": {
  }
}